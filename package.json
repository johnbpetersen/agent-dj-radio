{
  "name": "agent-dj-radio",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:api": "tsx dev-functions-server.ts",
    "dev:web": "vite",
    "dev:web:wait": "wait-on tcp:3001 && vite",
    "dev": "concurrently -k -n API,WEB -c magenta,blue \"npm:dev:api\" \"npm:dev:web:wait\"",
    "build": "vite build",
    "typecheck": "tsc -b",
    "lint": "eslint . --ext .ts,.tsx --max-warnings=0",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:smoke": "playwright test",
    "test:smoke:ui": "playwright test --ui",
    "format": "prettier -w .",
    "check:env": "tsx scripts/check-env.ts",
    "check:env:strict": "ALLOW_ENV_WARNINGS=false tsx scripts/check-env.ts",
    "env:settler:addr": "tsx scripts/derive-settler.ts",
    "doctor": "tsx scripts/doctor.ts",
    "ci:forbidden-refs": "bash scripts/ci-check-discord-refs.sh",
    "smoke:baseline": "bash scripts/smoke-baseline.sh",
    "x402:check-rpc": "tsx scripts/x402/check-rpc.ts",
    "x402:price-quote": "tsx scripts/x402/price-quote.ts",
    "prebuild": "npm run check:env:strict"
  },
  "dependencies": {
    "@coinbase/wallet-sdk": "^4.3.7",
    "@coinbase/x402": "^0.6.6",
    "@supabase/supabase-js": "^2.56.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tanstack/react-query": "^5.90.2",
    "@types/pg": "^8.15.5",
    "@vercel/node": "^5.3.20",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.545.0",
    "pg": "^8.16.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "viem": "^2.38.0",
    "wagmi": "^2.17.5",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.2",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "msw": "^2.10.5",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.20.5",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "vitest": "^3.2.4",
    "wait-on": "^8.0.4"
  }
}
