# Staging Environment Configuration
NODE_ENV=staging

# App
VITE_SITE_URL=https://agent-dj-radio-staging.vercel.app

# Supabase (Staging Project)
VITE_SUPABASE_URL=https://your-staging-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-staging-anon-key
SUPABASE_URL=https://your-staging-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-staging-service-role-key

# Feature flags (keep all services OFF for staging smoke tests)
ENABLE_REAL_ELEVEN=false
ENABLE_X402=false

# Admin Controls (required for staging smoke tests)
ADMIN_TOKEN=your-staging-admin-token-32-chars-minimum

# Optional Services (not used when flags are false, but documented)
# ELEVEN_API_KEY=
# ELEVEN_MUSIC_MODEL_ID=eleven_music_v1
# X402_PROVIDER_URL=https://api.cdp.coinbase.com/x402
# X402_ACCEPTED_ASSET=USDC
# X402_CHAIN=base-sepolia
# X402_RECEIVING_ADDRESS=

# Logging Configuration
LOG_LEVEL=info
ENABLE_REQUEST_LOGGING=true

# Error Tracking
ENABLE_ERROR_TRACKING=true